# Blockchain API Configuration
# Copy this file to .env and fill in your API keys

# Alchemy API for Ethereum blockchain data
ALCHEMY_API_KEY=your_alchemy_api_key_here
ALCHEMY_NETWORK=eth-mainnet

# Infura as backup provider
INFURA_PROJECT_ID=your_infura_project_id_here

# Etherscan API for additional blockchain data
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Price Data APIs
COINGECKO_API_KEY=your_coingecko_api_key_here
COINMARKETCAP_API_KEY=your_coinmarketcap_api_key_here

# MEV Data Sources
FLASHBOTS_API_URL=https://relay.flashbots.net
FLASHBOTS_REPUTATION_URL=https://relay-analytics.flashbots.net

# DEX Data Sources
UNISWAP_V3_SUBGRAPH_URL=https://api.thegraph.com/subgraphs/name/uniswap/uniswap-v3
SUSHISWAP_SUBGRAPH_URL=https://api.thegraph.com/subgraphs/name/sushiswap/exchange

# Caching Configuration
CACHE_TTL_PRICES=60000
CACHE_TTL_BLOCKCHAIN=30000
CACHE_TTL_MEV=300000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# WebSocket Configuration
WS_PORT=8081

# Server Configuration
NODE_ENV=development
LOG_LEVEL=info