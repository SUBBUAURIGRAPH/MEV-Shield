{
  "name": "mev-shield",
  "version": "1.3.0",
  "description": "MEV Shield - Complete MEV protection platform with Uniswap integration",
  "main": "index.js",
  "scripts": {
    "test": "hardhat test",
    "test:uniswap": "hardhat test test/uniswap.test.js",
    "compile": "hardhat compile",
    "deploy": "hardhat run scripts/deploy.js",
    "deploy:testnet": "hardhat run scripts/deploy.js --network goerli",
    "frontend": "cd dashboard && npm start",
    "backend": "cd backend-mock && npm start",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "build": "cd dashboard && npm run build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,sol}\"",
    "coverage": "hardhat coverage",
    "verify": "hardhat verify",
    "jira:sync": "gh workflow run jira-bidirectional-sync.yml"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.0.0",
    "@uniswap/sdk-core": "^4.0.1",
    "@uniswap/smart-order-router": "^3.15.0",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.3",
    "@uniswap/v3-sdk": "^3.9.0",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^6.15.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "web3": "^4.2.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@typechain/ethers-v6": "^0.5.0",
    "@typechain/hardhat": "^9.0.0",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "chai": "^4.3.10",
    "concurrently": "^8.2.1",
    "eslint": "^8.50.0",
    "hardhat": "^2.19.0",
    "hardhat-gas-reporter": "^1.0.9",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-solidity": "^1.1.3",
    "solidity-coverage": "^0.8.5",
    "typescript": "^5.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SUBBUAURIGRAPH/MEV-Shield.git"
  },
  "keywords": [
    "MEV",
    "DeFi",
    "Ethereum",
    "Uniswap",
    "Smart Contracts",
    "Blockchain",
    "Web3",
    "MEV Protection",
    "DEX"
  ],
  "author": "MEV Shield Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SUBBUAURIGRAPH/MEV-Shield/issues"
  },
  "homepage": "https://github.com/SUBBUAURIGRAPH/MEV-Shield#readme"
}
